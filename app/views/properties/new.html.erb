<%= form_for @property do |form| %>
  <div class = "property">
    <%= form.label :物件名 %>
    <%= form.text_field :name %>
  </div>

  <div class="property">
    <%= form.label :賃料 %>
    <%= form.text_field :money %>
  </div>

  <div class="property">
    <%= form.label :住所 %>
    <%= form.text_field :address %>
  </div>

  <div class="property">
    <%= form.label :築年数 %>
    <%= form.text_field :age %>
  </div>

  <div class="property">
    <%= form.label :備考 %>
    <%= form.text_area :content %>
  </div>

<%= form.submit :登録する %>

<% end %>
